{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/.auth/*",
      "/assets/*",
      "/*.css",
      "/*.js",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.gif",
      "/*.svg",
      "/*.ico",
      "/api/*"
    ]
  },
  "routes": [
    {
      "route": "/dashboard",
      "allowedRoles": ["authenticated"],
      "statusCode": 302,
      "redirect": "/login"
    },
    {
      "route": "/profile",
      "allowedRoles": ["authenticated"],
      "statusCode": 302,
      "redirect": "/login"
    },
    {
      "route": "/rewards",
      "allowedRoles": ["authenticated"],
      "statusCode": 302,
      "redirect": "/login"
    },
    {
      "route": "/challenges",
      "allowedRoles": ["authenticated"],
      "statusCode": 302,
      "redirect": "/login"
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/login",
      "statusCode": 302
    },
    "403": {
      "redirect": "/login",
      "statusCode": 302
    }
  }
}
